{
  "name": "oh-designie-game-guide",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "lint-staged": "lint-staged",
    "tslint": "tslint './src/**/*.ts{,x}'",
    "precommit": "npm-run-all -p lint-staged tslint"
  },
  "dependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-scripts-ts": "2.16.0"
  },
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "@types/node": "^10.3.1",
    "@types/react": "^16.3.16",
    "@types/react-dom": "^16.0.6",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.4",
    "typescript": "^2.9.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  }
}
